<%= simple_form_for @winner, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :nameteam %>
  <%= f.input :namemember %>
  <%= f.input :score %>
  <%= f.input :dategame %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                winners_path, :class => 'btn' %>
  </div>
<% end %>
