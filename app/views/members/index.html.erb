<%- model_class = Member -%>
<div class="page-header">
  <h1>Miembros: <%= @team.name %></h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>nombre</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @members %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to image_tag("participantes.png"), new_team_member_path, :remote => true  %> </td>
      <td><%= link_to image_tag("atras.png"), teams_path %></td>
    </tr>
  </tfoot>
</table>

