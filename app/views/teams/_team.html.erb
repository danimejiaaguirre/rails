<tr id=<%= "team_#{team.id}" %>
  <td></td>
  <td><%= link_to team.id, edit_team_path(team), :remote => true %></td>
  <td><%= team.name %></td>
  <td><%= team.score %></td>
  <td>
  <%= link_to  image_tag('mostrar.png'), team,     :remote => true, :class => 'btn btn-mini' %>

    <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_team_path(team),  :remote => true,
        :class => 'btn btn-mini'%>
    <%= link_to image_tag('eliminar.png', :title => "Eliminar Equipos"), team, :data => { :confirm => 'Esta Seguro?' },
        :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
        <td><%= link_to image_tag('miembros.png', :tile => "Agregar Miembros"), team_members_path(team) %></td>
  </td>
</tr>
  
         
            
             
              
       