<%- model_class = Answer-%>
<div class="page-header">
  <h1>Listado de Respuestas de: <%= @question.description %></h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>responder</th>
    </tr>
  </thead>
  <tbody>
     <%= render @answers %>
  </tbody>
  <tfoot>
    <tr>
     
      <td><%= link_to 'Nuevo Respuestas', new_question_answer_path, :remote => true  %> </td>
      <td><%= link_to 'Atras', questions_path %></td>
    </tr>
  </tfoot>
</table>

